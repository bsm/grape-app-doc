<% unless entities.empty? -%>
# Entities

<% entities.each do |entity| -%>
<%= entity.name %>

<% end -%>
<% end -%>

<% endpoints.group_by(&:version).each do |version, endpoints| -%>
# Endpoints <%= "<sup>#{version}</sup>" if version %>

<% endpoints.each do |endpoint| -%>
<%= endpoint.path %>

<% end -%>
<% end -%>
